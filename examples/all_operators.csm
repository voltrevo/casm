// Program with all operators
i32 main() {
    i32 a = 10;
    i32 b = 5;
    i32 c = a + b;
    i32 d = a - b;
    i32 e = a * b;
    i32 f = a / b;
    i32 g = a % b;
    
    if (a == 10 && b == 5) {
        return 1;
    } else if (a != b || a > b) {
        return 2;
    }
    
    return 0;
}
